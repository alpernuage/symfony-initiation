{% extends 'base.html.twig' %}
{% block title %}{{ 'actions.edit'|trans({'%entity%': 'labels.home'|trans}) }}{% endblock %}
{% block body %}
    <section>
        {% include '_layout/page_title.html.twig' with {'page_title': 'actions.edit'|trans({'%entity%': 'labels.home'|trans}) } %}
    </section>

    <section class="mb-4">
        {{ form(edit_home_form) }}
    </section>

    {% include '_layout/return_button.html.twig' with {
        'path': path('home_list'),
        'return_button': 'actions.cancel'|trans({'%entity%': 'labels.home'|trans})
    } %}
{% endblock %}
